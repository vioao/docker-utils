{
  "inbounds": [{
    "port": ${port},
    "protocol": "vmess",
    "settings": {
      "clients": [
        {
          "id": ${id},
          "level": 1,
          "alterId": ${alterId}
        }
      ]
    }
  }],
  "outbounds": [{
    "protocol": "freedom",
    "settings": {}
  },{
    "protocol": "blackhole",
    "settings": {},
    "tag": "blocked"
  }],
  "routing": {
    "rules": [
      {
        "type": "field",
        "ip": ["geoip:private"],
        "outboundTag": "blocked"
      }
    ]
  }
}
